/**
 * mod - finds modulus for two numbers
 * @a: first number
 * @b: second number
 * Return: number
 */
int mod(int a, int b)
{
return (a % b);
}

/**
 * mul - multiplies two numbers
 * @a: first number
 * @b: seconde number
 * Return: number
 */
int mul(int a, int b)
{
return (a * b);
}

/**
 * div - divides two numbers
 * @a: first number
 * @b: seconde number
 * Return: number
 */
int div(int a, int b)
{
return (a / b);
}


/**
 * add - adds two numbers
 * @a: first number
 * @b: seconde number
 * Return: number
 */
int add(int a, int b)
{
return (a + b);
}

/**
 * mul - subtracts two numbers
 * @a: first number
 * @b: seconde number
 * Return: number
 */
int sub(int a, int b)
{
return (a - b);
}
